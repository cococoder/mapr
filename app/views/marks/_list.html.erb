<h1>Marks</h1>

<table>
  <thead>
  <tr>
    <th>Context</th>
    <th>uid</th>
    <th>Left</th>
    <th>Top</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% source.each do |mark| %>
    <tr>
      <td><%= mark.context %></td>
      <td><%= mark.uid %></td>
      <td><%= mark.left %></td>
      <td><%= mark.top %></td>
      <td><%= link_to 'Show', mark %></td>
      <td><%= link_to 'Edit', edit_mark_path(mark) %></td>
      <td><%= link_to 'Destroy', mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>